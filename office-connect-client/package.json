{
  "name": "office-connect-client",
  "version": "1.3.4",
  "description": "Lan only communication app",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "build": {
    "appId": "com.office.connect",
    "productName": "Office Connect",
    "publish": [
      {
        "provider": "generic",
        "url": "http://192.168.1.50:5171/updates/"
      }
    ],
    "icon": "build/icons/icon",
    "win": {
      "icon": "build/icons/icon.ico",
      "target": "nsis"
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "linux": {
      "icon": "build/icons/icon.png"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "nodemon --watch src --ext js,jsx --exec node electron-dev.js",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@microsoft/signalr": "^10.0.0",
    "@reduxjs/toolkit": "^2.11.1",
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.13.2",
    "electron-log": "^5.4.3",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.6.2",
    "emoji-picker-react": "^4.16.1",
    "jwt-decode": "^4.0.0",
    "react-hook-form": "^7.68.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@types/node": "^25.0.2",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "vite": "^7.2.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  }
}
